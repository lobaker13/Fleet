<div id="home">
<h2>Search Results</h2>
	<% if @users %>
	<% @users.each do |user| %>
		<a href="/<%= user.username %>">
	<div class="search">
		<% if user.avatar.url != '/images/original/missing.png' %>
		<div class="searchImage" style="background-image: url('<%= user.avatar.url %>');">
		</div>
		<% end %>
		<h3><%= user.fullname %></h3>
	</div>
	</a>
	<% end %>
	<% else %>
		<h3>No results</h3>
	<% end %>
</div> 